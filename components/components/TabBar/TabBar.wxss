.FooTer{
  box-shadow: 0 0 60rpx #888888;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 130rpx;
  background: #fff;
  /* border: 1px solid green; */
  border-top-left-radius: 46rpx;
  border-top-right-radius: 46rpx;
  z-index: 997;

  bottom: calc(constant(safe-area-inset-bottom) /2);
  padding-bottom: calc(env(safe-area-inset-bottom) /2);
}
.FooTer .FTCont{
  display: flex;
  padding:0 50rpx;
  flex-direction: row;
}
.FooTer .FTCont .Normal{
  padding: 15rpx 0 17rpx;
  /* margin-bottom: 20rpx; */
  width:100%;
  text-align:center;
  /* border:1px solid green; */

}
.FooTer .FTCont .Normal .pic{
  width: 60rpx;
  height: 60rpx;
}
.FooTer .FTCont .Normal .txt{
  font-size: 28rpx;
  color: #999;
}

.FooTer .FTCont .Normal .txt_selected{
  font-size: 28rpx;
  color: #fed025;
}

.FooTer .FTCont .Circle{
  
  text-align: center;
  align-items: center;
  /* position: absolute; */
  /* border: 1px solid blue; */
}

.FooTer .FTCont .Circle .WCircle{
  box-shadow: 0px -10rpx 40rpx -20rpx #666;
  position: relative;
  top: -66rpx;
  background: #fff;
  width: 132rpx;
  height: 132rpx;
  border-radius:50%;


  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  z-index: 998;
  
}
