}
.Title .Titname .Tittxt1{
  font-size: 40rpx;
  font-weight: bold;
}
.Title .TitInfo{
  display:flex;
  flex-direction:row;
}
.Title .Titname .TitInfo .Tittxt2{
  margin-top: 20rpx;
  padding:2rpx 20rpx;
  font-size: 25rpx;
  color: rgb(207, 174, 27);
  background-color: #fde7b8;
  border-radius: 25rpx;
  text-align: center;
}
.Title .Titname .TitInfo .Tittxt3{
  margin-top: 20rpx;
  margin-left:15rpx;
  padding:2rpx 20rpx;
  font-size: 25rpx;
  color:#da8398;
  background-color: rgb(248, 207, 218);
