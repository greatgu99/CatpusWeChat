page{
  background-color:#fdfcea ;
}

.FormCont {
  justify-items: center;
  justify-content: space-between;
  display: flex;
  flex-direction:column;
}
.FormCont .Input{
  margin:30rpx;
  height:400rpx;
  background:#fff;
  border-radius:25rpx; 
}
.FormCont .Input .Count{
  position:relative;
  left:80%;
  top:20%;
  color:#cfcfcf;
}

.FormCont .Input .pic{
  margin:20rpx;
  width: 150rpx;
  height: 150rpx;
  background: #e6e6e6;
  display: flex;
  position: relative;
  top:36%;
  left:2%;
  align-items: center;
  justify-content: center;
  border-radius: 20rpx;
}

.FormCont .pic .pic-con{
  color:#888;
  font-size: 25rpx;
}
.FormCont .pic-img{
  margin:20rpx;
  width: 188rpx;
}
.FormCont .CatBtn{
  margin:20rpx 20rpx 30rpx;
  display: flex;
  background:#ffffff;
  justify-content: center;
  border-radius: 50rpx;
  align-items: center;
  width:400rpx;
}

.FormCont .CatBtn .ChoiceImage{
  height:60rpx;
  width: 400rpx;
}

.FormCont .CatBtn text{
  color: #ffe585;
  font-size:8;
}

.FormCont .Tab{
  background:#fdfcea;
  position:fixed;
  bottom:0;
  width:100%;
  height:150rpx;
  display:flex;
  justify-content: center;
  align-items:center;
}
.FormCont .Tab .FTbtn{
  height: 80rpx;
  width: 65%;
  display: flex;
  /* padding: 1rpx 20rpx 20rpx; */
  /* box-sizing: border-box; */
  background: #ffd22f;
  justify-content: center;
  border-radius: 50rpx;
  align-items: center;
}
.FormCont .Tab .FTbtn text{
  color: #fff;
  font-size:30rpx;
}

.warning-text{
  color:red;
  margin-top: 20rpx;
  margin-left:24rpx;
  margin-bottom: 10rpx;
  font-size: bolder;
}

.DynFT {
  display: flex;
  margin: 25rpx 40rpx;
  justify-content: space-between;
  padding: 15rpx;
  background: #fff;
  border-radius: 35rpx;
  align-items: center;
  /* border:1px solid green; */
  /* border:1px solid blue; */
}

.DynFT .FThead{
  display: flex;
}

.DynFT .FThead .FTpic{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.DynFT .FThead .FTtxt{
  margin-left: 15rpx;
}
.DynFT .FThead .FTtxt .FTtxt1{
  font-size: 32rpx;
  font-weight: bold;
}

.DynFT .FThead .FTtxt .FTtxt2{
  margin-top: 6rpx;
  font-size: 26rpx;
  color: #9c9c9c;
}

.weui-input{
  margin-top: 15rpx;
  margin-left: 20rpx;
}