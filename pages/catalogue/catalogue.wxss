page{
  background: #f3f3f3;
}


.Title{
  /* border:1px solid green; */
  padding:30rpx 20rpx;
  background:#fff;
  font-size: 40rpx;
  font-weight: bold;
}

.addCatBtn{
  border:1rpx solid #ffcb1e;
  height: 55rpx;
  width: 160rpx;
  background: #FFF;
  border-radius: 55rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 40rpx;
}
.addCatBtn text{
  font-size:25rpx;
  color:#ffcb1e;
}

.DynFT {
  display: flex;
  margin: 25rpx 40rpx;
  justify-content: space-between;
  padding: 15rpx;
  background: #fff;
  border-radius: 35rpx;
  align-items: center;
  /* border:1px solid green; */
  /* border:1px solid blue; */
}

.DynFT .FThead{
  display: flex;
}

.DynFT .FThead .FTpic{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.DynFT .FThead .FTtxt{
  margin-left: 15rpx;
}
.DynFT .FThead .FTtxt .FTtxt1{
  font-size: 32rpx;
  font-weight: bold;
}

.DynFT .FThead .FTtxt .FTtxt2{
  margin-top: 6rpx;
  font-size: 26rpx;
  color: #9c9c9c;
}
.DynFT .FTButton{
  display: flex;
}
.DynFT .FTButton .FTbtn1{
  height: 50rpx;
  width: 130rpx;
  border-radius: 50rpx;
  border:4rpx solid #ffe585;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
}
.DynFT .FTButton .FTbtn1 .FTtxt1{
  color: #ffe06e;
  font-size:25rpx;
}

.DynFT .FTButton .FTbtn2{
  margin-left: 6rpx;
  height: 50rpx;
  width: 80rpx;
}

.block{
  height: 190rpx;
  width: 100%;
}